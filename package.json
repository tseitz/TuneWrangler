{
  "name": "tunewrangler",
  "version": "0.9.0",
  "description": "Node application that wrangles my tunes.",
  "main": "common.js",
  "type": "module",
  "scripts": {
    "rM": "deno run --allow-read --allow-run --allow-write src/renameMusic.ts",
    "retag": "deno run --allow-read --allow-run --allow-write src/retagSongs.ts",
    "rI": "ts-node src/renameItunes.ts",
    "rB": "deno run --allow-read --allow-run --allow-net --allow-write --allow-env src/renameBeatport.ts",
    "rP": "ts-node src/renamePlaylists.ts",
    "cB": "node src/checkBitrate.js",
    "ytRb": "deno run --unstable --allow-env --allow-net --allow-read --allow-write --allow-run src/addRekordboxSongsToYoutube.ts",
    "ytRbNoCheck": "deno run --unstable --allow-env --allow-net --allow-read --allow-write --allow-run --no-check src/addRekordboxSongsToYoutube.ts",
    "cB2": "node --loader ts-node/esm src/checkBitrate.ts",
    "checkDupes": "ts-node src/checkDuplicates.ts",
    "analyze": "ts-node src/analyzePlaylists.ts",
    "checkCorrupted": "ts-node src/checkCorruptedPlaylists.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@types/node": "^18.11.18",
    "eslint": "^8.32.0",
    "node-id3": "^0.2.5",
    "prompt": "^1.3.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.94"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tseitz/TuneWrangler.git"
  },
  "author": "Tegan Seitz",
  "license": "ISC"
}
